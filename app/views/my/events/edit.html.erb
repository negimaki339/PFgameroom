マイチーム編集画面
<%= form_with model: [@team, @event],url:my_team_event_path(@team.id,@event.id), local:true do |f| %>
<div class="form-group">
    <%= f.label :イベント名 %>
    <%= f.text_field :event_name %>
  </div>
  <div class="form-group">
    <%= f.label :イベント説明文 %>
    <%= f.text_area :event_explanation %>
  </div>
  <div class="form-group">
    <%= f.label :イベント開始時刻 %>
    <%= f.datetime_field :time %>
  </div>
  <div class="form-group">
    <%= f.submit "編集完了", class: 'btn btn-success' %>
  </div>

<% end %>