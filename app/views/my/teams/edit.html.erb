マイチーム編集画面
<%= form_with model:@team,local:true do |f| %>
<div class="form-group">
    <%= f.label :ゲーム名 %>
    <%= f.text_field :game_name %>
  </div>
  <div class="form-group">
    <%= f.label :チーム名 %>
    <%= f.text_field :team_name %>
  </div>
  <div class="form-group">
    <%= f.label :チーム募集文 %>
    <%= f.text_area :team_explanation %>
  </div>
  <div class="form-group">
    <%= f.label :チーム概要 %>
    <%= f.text_area :overview %>
  </div>
    <%= f.radio_button :is_join, 'free'  %>
    <%= f.label :is_join, "自由加入制" %>
    <%= f.radio_button :is_join, 'approval' %>
    <%= f.label :is_join, "承認制" %>
  <div class="form-group">
    <%= f.submit "更新", class: 'btn btn-success' %>
  </div>

<% end %>
<%= link_to '削除', "/teams/#{@team.id}", method: :delete, data: { confirm: '本当にチームを削除しますか？' } %>